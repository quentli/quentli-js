{
  "name": "@quentli/js",
  "description": "Official JavaScript SDK for integrating Quentli on the web",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@10.12.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "MIGRATION.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "quentli",
    "payments",
    "checkout",
    "payment-sessions",
    "javascript",
    "typescript"
  ],
  "author": "Quentli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quentli/quentli-js.git"
  },
  "bugs": {
    "url": "https://github.com/quentli/quentli-js/issues"
  },
  "homepage": "https://github.com/quentli/quentli-js#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "semantic-release": "^23.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}